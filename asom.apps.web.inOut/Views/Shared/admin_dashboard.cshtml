@using System.Web.Optimization
@using asom.apps.web.inOut.Models
<!DOCTYPE html>
<html ng-app="ebaam">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>Dashboard</title>
    @Html.Partial("admin_header_resource_urls")
        

</head>
<body>

<div class="dashboard-main-wrapper">
<!-- ============================================================== -->
<!-- navbar -->
<!-- ============================================================== -->
@Html.Partial("ui/menu/top/_mainMenu")
<!-- ============================================================== -->
<!-- end navbar -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- left sidebar -->
<!-- ============================================================== -->
@Html.Partial("ui/menu/left/_leftMenu")
<!-- ============================================================== -->
<!-- end left sidebar -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- wrapper  -->
<!-- ============================================================== -->
<div class="dashboard-wrapper">
<div class="dashboard-ecommerce">
<div class="container-fluid dashboard-content ">
<!-- ============================================================== -->
<!-- pageheader  -->
<!-- ============================================================== -->
    <asom-route-page-details></asom-route-page-details>
<!-- ============================================================== -->
<!-- end pageheader  -->
<!-- ============================================================== -->
<div class="ecommerce-widget" style="min-width: 400px;">
    <ui-view name="" class="animated anim-ui-view" @*autoscroll="true"*@>

        @RenderBody()

    </ui-view>

    

</div>
</div>
</div>
<!-- ============================================================== -->
<!-- footer -->
<!-- ============================================================== -->
<div class="footer">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                Copyright © @DateTime.Now.Year [App Name]. All rights reserved.
            </div>
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                <div class="text-md-right footer-links d-none d-sm-block">
                    <a href="javascript: void(0);">About</a>
                    <a href="javascript: void(0);">Support</a>
                    <a href="javascript: void(0);">Contact Us</a>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ============================================================== -->
<!-- end footer -->
<!-- ============================================================== -->
</div>
<!-- ============================================================== -->
<!-- end wrapper  -->
<!-- ============================================================== -->
</div>
<script src="~/Content/assets/vendor/jquery/jquery-3.3.1.min.js"></script>
<script src="~/Content/assets/vendor/slimscroll/jquery.slimscroll.js"></script>
<script src="~/Content/assets/vendor/bootstrap/js/bootstrap.min.js"></script>
<script src="~/Content/assets/libs/js/main-js.js"></script>

@Scripts.Render("~/required/components")
@Scripts.Render("~/required/libs")
@Scripts.Render("~/inout/lib")
@Html.Partial("ui/_urls")
@RenderSection("scripts", required: false)
</body>
`ll
</html>